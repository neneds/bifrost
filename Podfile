target 'Bifrost' do
  platform :ios, '11.3'
  use_frameworks!
  # Pods for BYODLegacyMedia
  pod 'Moya', '~> 12.0'

  target 'BifrostTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
  end

end
